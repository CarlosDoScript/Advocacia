<script>
    //Menu Lateral Tab Icons ativos
    $(document).ready(function () {
        var path = window.location.pathname;
        var page = path.split("/").pop();

        var tabs = ['menu-fila-de-espera', 'menu-chat'];

        for (var i = 0, len = (tabs.length - 1); i <= len; ++i) {
            $('#' + tabs[i]).removeClass('active')
        }
        if (page == 'Contact') {
            $(".tituloPagina").text("Advocacia - Contato");
            $('.menu-contact').addClass('active')
        }
        else if (page == 'Index') {
            $(".tituloPagina").text("Advocacia - Home");
            $('.menu-principal').addClass('active')
        }
        else if (page == 'PracticeArea') {
            $(".tituloPagina").text("Advocacia - Área de prática");
            $('.menu-areaPratica').addClass('active')
        }
        else if (page == 'Blog' || page == 'Finalidades' || page == 'Postagens' || page == 'Postagem') {
            $(".tituloPagina").text("Advocacia - Blog");
            $('.menu-blog').addClass('active')
        } else {
            $(".tituloPagina").text("Advocacia - Home");
            $('.menu-principal').addClass('active')
        }

    });
</script>