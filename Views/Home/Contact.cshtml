                        <h2 class="section-heading">
                            Envie-nos uma mensagem
                        </h2>
                        <p class="subheading mb-4">"sempre cuidamos dos clientes com seriedade"</p>
                        @using (Html.BeginForm("Contact", "Home", FormMethod.Post, new { id = "formContato", name = "formContato" }))
                        {
                            @Html.AntiForgeryToken()
                            <div class="row">
                                <div class="col-sm-6 col-md-6">
                                    <div class="form-group">
                                        @Html.TextBoxFor(x => x.Nome, new { @class = "form-control", placeholder = "Insira o nome completo" })
                                        <div class="help-block with-errors"></div>
                                    </div>
                                </div>
                                <div class="col-sm-6 col-md-6">
                                    <div class="form-group">
                                        @Html.TextBoxFor(x => x.Email, new { @class = "form-control", placeholder = "Digite o e-mail" })
                                        <div class="help-block with-errors"></div>
                                    </div>
                                </div>
                                <div class="col-sm-6 col-md-6">
                                    <div class="form-group">
                                        @Html.TextBoxFor(x => x.Celular, new { @class = "form-control", placeholder = "Digite o número de telefone ou celular" })
                                        <div class="help-block with-errors"></div>
                                    </div>
                                </div>
                                <div class="col-sm-6 col-md-6">
                                    <div class="form-group">
                                        @Html.TextBoxFor(x => x.Titulo, new { @class = "form-control", placeholder = "Título" })
                                        <div class="help-block with-errors"></div>
                                    </div>
                                </div>
                            </div>
                            <div class="form-group">
                                @Html.TextAreaFor(x => x.Mensagem, new { @class = "form-control",rows="6", placeholder = "Digite sua mensagem" })
                                <div class="help-block with-errors"></div>
                            </div>
                            <div class="form-group">
                                <div id="success"></div>
                                <button type="submit" class="btn btn-secondary">Enviar Mensagem</button>
                            </div>
                        }
